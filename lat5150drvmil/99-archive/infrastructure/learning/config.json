{
  "database": {
    "host": "localhost",
    "port": 5433,
    "database": "claude_auth",
    "user": "claude_auth",
    "password": "claude_auth_pass",
    "min_connections": 5,
    "max_connections": 20,
    "timeout": 30
  },
  "dsmil": {
    "monitor_interval": 1.0,
    "thermal_warning": 75,
    "thermal_critical": 85,
    "device_timeout": 5.0,
    "shadowgit_processing_rate": 930000000,
    "simd_optimization": true
  },
  "ml": {
    "embedding_dimensions": 512,
    "similarity_threshold": 0.8,
    "batch_size": 100,
    "learning_rate": 0.001,
    "anomaly_contamination": 0.1,
    "clustering_eps": 0.3,
    "clustering_min_samples": 5
  },
  "agents": {
    "max_concurrent": 10,
    "default_timeout": 30.0,
    "retry_attempts": 3,
    "performance_weight": 0.7,
    "speed_weight": 0.3,
    "emergency_boost": 1.5,
    "high_priority_boost": 1.2
  },
  "performance": {
    "simd_enabled": true,
    "sse42_optimized": true,
    "vector_operations": true,
    "async_processing": true,
    "concurrent_analysis": true,
    "max_workers": 4
  },
  "monitoring": {
    "health_check_interval": 60,
    "stalled_task_timeout": 300,
    "agent_reset_timeout": 300,
    "performance_history_days": 30,
    "alert_thresholds": {
      "critical_temperature": 85,
      "high_error_count": 5,
      "performance_degradation": 0.5
    }
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "max_history": 1000,
    "log_performance": true,
    "log_ml_operations": true
  }
}