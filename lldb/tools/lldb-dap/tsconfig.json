{
	"compilerOptions": {
		"moduleResolution": "node",
		"module": "commonjs",
		"outDir": "out",
		"rootDirs": ["src-ts", "unittests-ts"],
		"sourceMap": true,
		"strict": true,
		"target": "es6"
	},
	"include": [
		"src-ts",
		"unittests-ts"
	],
	"exclude": [
		"node_modules",
		"src-ts/webview",
	]
}
