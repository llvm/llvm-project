  - { id: 0, class: vgpr_32, preferred-register: $vgpr1, flags: [ WWM_REG ]}
  - { id: 1, class: sgpr_64, preferred-register: $sgpr0_sgpr1 }
  - { id: 2, class: sgpr_64, flags: [ ] }
