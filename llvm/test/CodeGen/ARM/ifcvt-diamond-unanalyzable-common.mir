  ; CHECK-NEXT:   t2Bcc %bb.2, 1 /* CC::ne */, $cpsr
  ; CHECK-NEXT:   t2Bcc %bb.2, 2 /* CC::hs */, killed $cpsr
  ; CHECK-NEXT:   t2B %bb.1, 14 /* CC::al */, $noreg
