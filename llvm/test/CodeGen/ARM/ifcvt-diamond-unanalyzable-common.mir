    t2B %bb.3, 14, $noreg

  bb.3:
    INLINEASM &"", 1
    $sp = t2LDMIA_RET $sp, 14, $noreg, def $r4, def $pc

  bb.4.l_yes (ir-block-address-taken %ir-block.l_yes):
    $sp = t2LDMIA_RET $sp, 14, $noreg, def $r4, def $pc

