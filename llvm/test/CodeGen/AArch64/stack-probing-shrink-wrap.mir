  define void @f(i32 %n) #0 {
  entry:
    %a = alloca i8, i32 150000, align 8
    %c0 = icmp sle i32 %n, 1
    br i1 %c0, label %if.then1, label %exit

  if.then1:                                         ; preds = %entry
