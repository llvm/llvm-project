    %v = select i1 %0, i32 0, i32 1
    call void @g(ptr %a, i32 %v)
    br label %exit

  exit:                                             ; preds = %if.then1, %entry
    ret void
  }

  declare void @g(...)

  attributes #0 = { nounwind "probe-stack"="inline-asm" "stack-probe-size"="4096" }

