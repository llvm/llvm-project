    ; CHECK-NEXT: $x0 = COPY [[OR]](s64)
    ; CHECK-NEXT: $x1 = COPY [[OR1]](s64)
    ; CHECK-NEXT: RET_ReallyLR implicit $x0, implicit $x1
    %0:_(s64) = COPY $x0
    %1:_(s64) = COPY $x1
    %2:_(s64) = COPY $x2
    %3:_(s64) = COPY $x3
    %4:_(s128) = G_MERGE_VALUES %0, %1
