    renamable $vgpr2_vgpr3 = DS_READ2_B32 renamable $vgpr13, 2, 3, 0, implicit $m0, implicit $exec
    renamable $vgpr1 = V_OR_B32_e32 1, killed $vgpr1, implicit $exec
