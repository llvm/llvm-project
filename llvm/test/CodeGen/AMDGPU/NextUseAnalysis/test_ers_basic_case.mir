--- |
  define amdgpu_ps i64 @test(ptr addrspace(3) %p1, ptr addrspace(3) %p2, i1 %cond1, i64 %val) #0 {
  entry:
    %cond1.inv = xor i1 %cond1, true
    %0 = call { i1, i32 } @llvm.amdgcn.if.i32(i1 %cond1.inv)
    %1 = extractvalue { i1, i32 } %0, 0
    %2 = extractvalue { i1, i32 } %0, 1
    br i1 %1, label %bb2, label %Flow
  
  Flow:                                             ; preds = %bb2, %entry
    %3 = phi i64 [ %ld2, %bb2 ], [ poison, %entry ]
    %4 = call { i1, i32 } @llvm.amdgcn.else.i32.i32(i32 %2)
    %5 = extractvalue { i1, i32 } %4, 0
    %6 = extractvalue { i1, i32 } %4, 1
    br i1 %5, label %bb1, label %exit
  
  bb1:                                              ; preds = %Flow
    %ld1 = load i64, ptr addrspace(3) %p1, align 1
    br label %exit, !amdgpu.uniform !0
  
  bb2:                                              ; preds = %entry
    %ld2 = load i64, ptr addrspace(3) %p2, align 1
    br label %Flow, !amdgpu.uniform !0
  
  exit:                                             ; preds = %bb1, %Flow
    %phi = phi i64 [ %3, %Flow ], [ %ld1, %bb1 ]
    call void @llvm.amdgcn.end.cf.i32(i32 %6)
    %add = add i64 %phi, %val
    ret i64 %add
  }
  
  ; Function Attrs: nocallback nofree nounwind willreturn
  declare { i1, i32 } @llvm.amdgcn.if.i32(i1) #1
  
  ; Function Attrs: nocallback nofree nounwind willreturn
  declare { i1, i32 } @llvm.amdgcn.else.i32.i32(i32) #1
  
  ; Function Attrs: nocallback nofree nounwind willreturn
  declare void @llvm.amdgcn.end.cf.i32(i32) #1
  
  attributes #0 = { "target-cpu"="gfx1200" }
  attributes #1 = { nocallback nofree nounwind willreturn }
  
  !0 = !{}
...
