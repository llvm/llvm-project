  - { id: 1, class: vgpr_32, preferred-register: '', flags: [  ] }
  - { id: 0, class: vgpr_32, preferred-register: '', flags: [  ] }
  - { id: 3, class: sreg_32, preferred-register: '', flags: [  ] }
  - { id: 2, class: vgpr_32, preferred-register: '', flags: [  ] }
