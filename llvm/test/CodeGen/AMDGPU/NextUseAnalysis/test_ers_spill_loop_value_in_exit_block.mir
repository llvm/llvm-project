  define amdgpu_ps void @test(ptr addrspace(1) %p1, ptr addrspace(1) %p2, ptr addrspace(1) %p3, ptr addrspace(1) %p4, ptr addrspace(1) %p5, i32 %TC) #0 {
  entry:
    %ld1 = load i32, ptr addrspace(1) %p2, align 4
    store i32 %TC, ptr addrspace(1) %p2, align 4
    br label %loop, !amdgpu.uniform !0
  
  loop:                                             ; preds = %loop, %entry
    %phi.broken = phi i32 [ %1, %loop ], [ 0, %entry ]
    %lsr.iv = phi i32 [ -1, %entry ], [ %lsr.iv.next, %loop ]
    %lsr.iv.next = add i32 %lsr.iv, 1
    %0 = add i32 %lsr.iv.next, 1
    %cond = icmp uge i32 %0, %ld1
    %1 = call i32 @llvm.amdgcn.if.break.i32(i1 %cond, i32 %phi.broken)
    %2 = call i1 @llvm.amdgcn.loop.i32(i32 %1)
    br i1 %2, label %exit, label %loop
  
  exit:                                             ; preds = %loop
    %lsr.iv.next.lcssa = phi i32 [ %lsr.iv.next, %loop ]
    %.lcssa2 = phi i32 [ %0, %loop ]
    %.lcssa = phi i32 [ %1, %loop ]
    call void @llvm.amdgcn.end.cf.i32(i32 %.lcssa)
    %sext.le = sext i32 %lsr.iv.next.lcssa to i64
    %gep.le = getelementptr inbounds i32, ptr addrspace(1) %p1, i64 %sext.le
    %ld.le = load i32, ptr addrspace(1) %gep.le, align 1
    %add.le = add i32 %ld.le, %lsr.iv.next.lcssa
    %ld2 = load i32, ptr addrspace(1) %p3, align 1
    %ld3 = load i32, ptr addrspace(1) %p4, align 4
    %add1 = add i32 %ld3, %.lcssa2
    %mul1 = mul i32 %ld3, %add1
    store i32 %mul1, ptr addrspace(1) %p3, align 4
    %sub1 = sub i32 %mul1, %add1
    %mul2 = mul i32 %sub1, %TC
    store i32 %mul2, ptr addrspace(1) %p4, align 4
    %mul3 = mul i32 %ld1, %ld2
    %add2 = add i32 %mul3, %add.le
    store i32 %add2, ptr addrspace(1) %p5, align 4
    ret void
  }
  
  ; Function Attrs: nocallback nofree nounwind willreturn memory(none)
