  %ld2 = load i32, ptr addrspace(1) %gep1, align 4
  %inc1 = add i32 %phi.inc1, 1
  %add2 = add i32 %ld2, %inc1
  %mul1 = mul i32 %ld2, %inc1
  store i32 %mul1, ptr addrspace(1) %p1, align 2
