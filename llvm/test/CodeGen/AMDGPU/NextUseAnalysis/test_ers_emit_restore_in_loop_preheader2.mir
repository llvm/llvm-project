  target datalayout = "e-m:e-p:64:64-p1:64:64-p2:32:32-p3:32:32-p4:64:64-p5:32:32-p6:32:32-p7:160:256:256:32-p8:128:128:128:48-p9:192:256:256:32-i64:64-v16:16-v24:32-v32:32-v48:64-v96:128-v192:256-v256:256-v512:512-v1024:1024-v2048:2048-n32:64-S32-A5-G1-ni:7:8:9"
  target triple = "amdgcn"
  
  define amdgpu_ps i32 @test(ptr addrspace(1) %p1, ptr addrspace(1) %p2, ptr addrspace(1) %p3, i32 %TC1, i32 %TC2) #0 {
  entry:
    %ld1 = load i32, ptr addrspace(1) %p1, align 1
    br label %loop1, !amdgpu.uniform !0
  
  loop1:                                            ; preds = %loop1, %entry
    %phi.broken = phi i32 [ %3, %loop1 ], [ 0, %entry ]
    %lsr.iv2 = phi i32 [ 0, %entry ], [ %lsr.iv.next3, %loop1 ]
    %lsr.iv = phi i32 [ 1, %entry ], [ %lsr.iv.next, %loop1 ]
    %phi1 = phi i32 [ %ld1, %entry ], [ %1, %loop1 ]
    %phi2 = phi i32 [ 100, %entry ], [ %mul1, %loop1 ]
    %phi3 = phi i32 [ %ld1, %entry ], [ %2, %loop1 ]
    %0 = add i32 %lsr.iv, -1
    %sext1 = sext i32 %0 to i64
    %gep1 = getelementptr inbounds i32, ptr addrspace(1) %p2, i64 %sext1
    %ld2 = load i32, ptr addrspace(1) %gep1, align 4
    %1 = add i32 %lsr.iv, %ld2
    %mul1 = mul i32 %ld2, %lsr.iv
    store i32 %mul1, ptr addrspace(1) %p1, align 2
    %gep2 = getelementptr inbounds i32, ptr addrspace(1) %p3, i64 %sext1
    %ld3 = load i32, ptr addrspace(1) %gep2, align 2
    %lsr.iv.next = add i32 %lsr.iv, 1
    %lsr.iv.next3 = add i32 %lsr.iv2, -1
    %2 = add i32 %lsr.iv2, %ld3
    %cond1 = icmp uge i32 %lsr.iv, %TC1
    %3 = call i32 @llvm.amdgcn.if.break.i32(i1 %cond1, i32 %phi.broken)
    %4 = call i1 @llvm.amdgcn.loop.i32(i32 %3)
    br i1 %4, label %bb, label %loop1
  
  bb:                                               ; preds = %loop1
    %phi1.lcssa = phi i32 [ %phi1, %loop1 ]
    %phi2.lcssa = phi i32 [ %phi2, %loop1 ]
    %phi3.lcssa = phi i32 [ %phi3, %loop1 ]
    %ld2.lcssa = phi i32 [ %ld2, %loop1 ]
    %mul1.lcssa = phi i32 [ %mul1, %loop1 ]
    %lsr.iv.next.lcssa = phi i32 [ %lsr.iv.next, %loop1 ]
    %.lcssa5 = phi i32 [ %3, %loop1 ]
    call void @llvm.amdgcn.end.cf.i32(i32 %.lcssa5)
    %5 = add i32 %lsr.iv.next.lcssa, -2
    %mul2.le = mul i32 %mul1.lcssa, %5
    %mul3 = mul i32 %phi1.lcssa, 100
    store i32 %mul3, ptr addrspace(1) %p3, align 4
    br label %loop2, !amdgpu.uniform !0
  
  loop2:                                            ; preds = %loop2, %bb
    %phi.broken4 = phi i32 [ %6, %loop2 ], [ 0, %bb ]
    %phi.inc2 = phi i32 [ 0, %bb ], [ %inc2, %loop2 ]
    %phi4 = phi i32 [ %phi3.lcssa, %bb ], [ %mul2.le, %loop2 ]
    %inc2 = add i32 %phi.inc2, 2
    %cond2 = icmp uge i32 %inc2, %TC2
    %6 = call i32 @llvm.amdgcn.if.break.i32(i1 %cond2, i32 %phi.broken4)
    %7 = call i1 @llvm.amdgcn.loop.i32(i32 %6)
    br i1 %7, label %exit, label %loop2
  
  exit:                                             ; preds = %loop2
    %phi4.lcssa = phi i32 [ %phi4, %loop2 ]
    %inc2.lcssa = phi i32 [ %inc2, %loop2 ]
    %.lcssa = phi i32 [ %6, %loop2 ]
    call void @llvm.amdgcn.end.cf.i32(i32 %.lcssa)
    %add1 = add i32 %ld1, 100
    store i32 %phi4.lcssa, ptr addrspace(1) %p3, align 4
    %add3.le = add i32 %phi4.lcssa, %inc2.lcssa
    %add4 = add i32 %add3.le, %phi2.lcssa
    %add5 = add i32 %add4, %mul3
    %add6 = add i32 %add5, %ld2.lcssa
    %add7 = add i32 %add6, %add1
    ret i32 %add7
  }
  
  ; Function Attrs: nocallback nofree nounwind willreturn memory(none)
  declare i32 @llvm.amdgcn.if.break.i32(i1, i32) #1
  
  ; Function Attrs: nocallback nofree nounwind willreturn
  declare i1 @llvm.amdgcn.loop.i32(i32) #2
  
  ; Function Attrs: nocallback nofree nounwind willreturn
  declare void @llvm.amdgcn.end.cf.i32(i32) #2
  
  attributes #0 = { "target-cpu"="gfx1200" }
  attributes #1 = { nocallback nofree nounwind willreturn memory(none) }
  attributes #2 = { nocallback nofree nounwind willreturn }
  
  !0 = !{}
...
