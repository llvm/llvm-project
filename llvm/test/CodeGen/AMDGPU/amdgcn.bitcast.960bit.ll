  %a1 = fadd <60 x half> %a, splat (half 0xH0200)
  %a2 = bitcast <60 x half> %a1 to <60 x i16>
  br label %end

cmp.false:
  %a3 = bitcast <60 x half> %a to <60 x i16>
  br label %end

end:
  %phi = phi <60 x i16> [ %a2, %cmp.true ], [ %a3, %cmp.false ]
  ret <60 x i16> %phi
}
