    ; GCN-NEXT: s_set_vgpr_msb 0
    ; ASM-SAME:                                         ;  msbs: dst=0 src0=0 src1=0 src2=0
    ; GCN-NEXT: ds_add_rtn_u32 v0, v0, v0
    $vgpr0 = DS_ADD_RTN_U32_gfx9 $vgpr0, $vgpr0, 0, 0, implicit $exec

