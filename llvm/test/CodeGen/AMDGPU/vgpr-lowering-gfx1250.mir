    ; GCN-NEXT: s_set_vgpr_msb 1
    ; ASM-SAME:                                         ;  msbs: dst=0 src0=1 src1=0 src2=0
    ; GCN-NEXT: global_load_b32 v2, v[2:3] /*v[258:259]*/, off
