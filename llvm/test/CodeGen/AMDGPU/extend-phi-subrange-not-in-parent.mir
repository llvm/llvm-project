  ; CHECK-NEXT:   S_NOP 0, implicit %6.sub0
  ; CHECK-NEXT: {{  $}}
  ; CHECK-NEXT: bb.3:
  ; CHECK-NEXT:   successors: %bb.4(0x80000000)
  ; CHECK-NEXT: {{  $}}
  ; CHECK-NEXT:   S_NOP 0, implicit %6
  ; CHECK-NEXT: {{  $}}
  ; CHECK-NEXT: bb.4:
