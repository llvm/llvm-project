    $sgpr6_sgpr7 = S_AND_SAVEEXEC_B64 $vcc, implicit-def $exec, implicit-def $scc, implicit $exec
    S_CBRANCH_SCC1 %bb.5, implicit $scc
  bb.4:
    liveins: $sgpr6_sgpr7
