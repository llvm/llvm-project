  ; GCN-NEXT: {{  $}}
  ; GCN-NEXT:   $vcc = V_CMP_EQ_U32_e64 0, [[V_MOV_B32_e32_1]], implicit $exec
  ; GCN-NEXT:   $sgpr6_sgpr7 = S_AND_SAVEEXEC_B64 $vcc, implicit-def $exec, implicit-def $scc, implicit $exec
  ; GCN-NEXT:   S_CBRANCH_SCC1 %bb.5, implicit $scc
  ; GCN-NEXT: {{  $}}
  ; GCN-NEXT: bb.4:
