tracksRegLiveness: true
body:             |
  ; CHECK-LABEL: name: live_variables_update_block_split_non_phi_live_across
  ; CHECK: bb.0:
  ; CHECK-NEXT:   successors: %bb.3(0x40000000), %bb.1(0x40000000)
