  ; CHECK-NEXT: {{  $}}
  ; CHECK-NEXT:   [[PRED_COPY5a:%[0-9]+]]:vgpr_32 = PRED_COPY killed [[PRED_COPY5]]
  ; CHECK-NEXT:   GLOBAL_STORE_DWORD undef %11:vreg_64, [[PRED_COPY5a]], 0, 0, implicit $exec :: (volatile store (s32), addrspace 1)
  ; CHECK-NEXT:   [[COPY2:%[0-9]+]]:vgpr_32 = COPY killed [[PRED_COPY5a]]
  ; CHECK-NEXT:   [[PRED_COPY6:%[0-9]+]]:vgpr_32 = PRED_COPY [[COPY2]]
  ; CHECK-NEXT:   [[PRED_COPY6:%[0-9]+]]:vgpr_32 = PRED_COPY killed [[COPY2]]
