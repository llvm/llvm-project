  ; CHECK-NEXT:   [[PRED_COPY4:%[0-9]+]]:sreg_64_xexec = PRED_COPY killed [[S_MOV_B64_term]]
  ; CHECK-NEXT:   S_BRANCH %bb.2
  ; CHECK-NEXT: {{  $}}
