    Type:  SHT_PROGBITS
  - Name:  .llvm.callgraph
    Type:  SHT_LLVM_CALL_GRAPH
    ContentArray: [
      ## --- foo ---
      0x00, ## Format Version
      ##<Missing flags
    ]
Symbols:
  - Name:  foo
...

