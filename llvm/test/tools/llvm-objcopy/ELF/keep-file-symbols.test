#STRIPALL-NEXT:    Name: foo
#STRIPALL-NEXT:    Value: 0x0
#STRIPALL-NEXT:    Size: 0
#STRIPALL-NEXT:    Binding: Local
#STRIPALL-NEXT:    Type: File
#STRIPALL-NEXT:    Other: 0
#STRIPALL-NEXT:    Section: .text
#STRIPALL-NEXT:  }
#STRIPALL-NEXT:]

