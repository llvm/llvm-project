# RUN: yaml2obj %s -o %t
# RUN: not llvm-objcopy --build-id-link-dir=%t-dir --build-id-link-input=.debug %t 2>&1 >/dev/null | FileCheck %s -DINPUT=%t

# CHECK: error: '[[INPUT]]': could not find build ID

--- !ELF
FileHeader:
  Class:           ELFCLASS64
  Data:            ELFDATA2LSB
  Type:            ET_EXEC
  Machine:         EM_X86_64
Sections:
  - Name:            .note.foo
    Type:            SHT_NOTE
    Flags:           [ SHF_ALLOC ]
    Content:         000000000000000000000000
ProgramHeaders:
  - Type: PT_NOTE
    Flags: [ PF_R ]
    Sections:
      - Section: .note.foo
