# CHECK:    CU[0]: 0x00000000
# CHECK:  ]
# CHECK: Abbreviations
# CHECK: String: {{.*}} "_Z3foov"
# CHECK: Tag: DW_TAG_subprogram
# CHECK: String: {{.*}} "int"
# CHECK: Tag: DW_TAG_base_type
# CHECK: String: {{.*}} "foo"
# CHECK: Tag: DW_TAG_subprogram
# CHECK: String: {{.*}} "A"
# CHECK: Tag: DW_TAG_structure_type
# CHECK: String: {{.*}} "main"
# CHECK: Tag: DW_TAG_subprogram
# CHECK: String: {{.*}} "char"
# CHECK: Tag: DW_TAG_base_type
