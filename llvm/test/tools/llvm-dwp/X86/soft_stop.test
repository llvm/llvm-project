# clang++ -g -gsplit-dwarf -gdwarf-5 -fdebug-types-section -S main.cpp
#
#enum class DebugType {
#    INFO,
#    WARNING,
#    ERROR,
#    CRITICAL,
#    DEBUG
#} g = DebugType::INFO; 
#
#int main() {
#    return 0;
#}

