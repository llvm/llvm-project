  %inc69 = add nuw nsw i16 %i.166, 1
  %exitcond77.not = icmp eq i16 %inc69, 2
  br i1 %exitcond77.not, label %for.cond75.preheader, label %for.cond33.preheader

for.cond75.preheader:                             ; preds = %for.inc68
  unreachable
}

