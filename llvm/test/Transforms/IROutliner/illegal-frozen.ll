; CHECK-NEXT:    ret void
; CHECK:       next:
; CHECK-NEXT:    br label [[FIRST]]
;
entry:
  br label %first
first:
  %c = freeze ptr %a
  store i32 2, ptr %c, align 4
  store i32 3, ptr %b, align 4
  ret void
next:
  br label %first
}

