  %red = phi i16 [ 0, %entry ], [ %red.next, %loop ]
  %red.next = or i16 %red, 0
  %iv.next = add i64 %iv, 1
  %ec = icmp eq i64 %iv.next, %clamped
  br i1 %ec, label %exit, label %loop

