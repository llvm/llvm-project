RUN: split-file %s %t
RUN: llvm-symbolizer --filter-markup < %t/log > %t.out 2> %t.err
RUN: FileCheck %s --input-file=%t.out --match-full-lines \
RUN:   --implicit-check-not {{.}}
RUN: FileCheck %s --check-prefix=ERR -input-file=%t.err --match-full-lines

