# RUN: yaml-bench -canonical %s | FileCheck %s --strict-whitespace
# CHECK:      !!map {
# CHECK-NEXT:   ? !!str "simple key"
# CHECK-NEXT:   : !!map {
# CHECK-NEXT:     ? !!str "also simple"
# CHECK-NEXT:     : !!str "value",
# CHECK-NEXT:     ? !!str "not a\n  simple key"
# CHECK-NEXT:     : !!str "any\n  value",
# CHECK-NEXT:   },
# CHECK-NEXT: }

"simple key" : {
  "also simple" : value,
  ? "not a
  simple key" : "any
  value"
}
