import("//llvm/utils/unittest/unittest.gni")

unittest("ClangPseudoTests") {
  configs += [ "//llvm/utils/gn/build:clang_code" ]
  deps = [
    "//clang-tools-extra/pseudo/lib",
    "//clang/lib/Basic",
    "//clang/lib/Lex",
    "//clang/lib/Testing",
    "//llvm/lib/Support",
  ]
  include_dirs = [ "../include" ]
  sources = [
    "DirectiveTreeTest.cpp",
    "ForestTest.cpp",
    "GLRTest.cpp",
    "GrammarTest.cpp",
    "LRTableTest.cpp",
    "TokenTest.cpp",
  ]
}
