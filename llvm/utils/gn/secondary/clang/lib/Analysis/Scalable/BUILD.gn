  sources = [
    "ASTEntityMapping.cpp",
    "Model/BuildNamespace.cpp",
    "Model/EntityIdTable.cpp",
    "Model/EntityName.cpp",
    "TUSummary/ExtractorRegistry.cpp",
  ]
