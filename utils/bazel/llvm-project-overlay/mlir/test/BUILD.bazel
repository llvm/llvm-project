        "@LLVM_BUILD_EXAMPLES@": "0",
        "@MLIR_ENABLE_CUDA_CONVERSIONS@": "0",
        "@MLIR_ENABLE_CUDA_RUNNER@": "0",
        "@MLIR_ENABLE_ROCM_CONVERSIONS@": "0",
        "@MLIR_ENABLE_ROCM_RUNNER@": "0",
