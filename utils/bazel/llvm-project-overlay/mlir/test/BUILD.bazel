        "@MLIR_RUN_AMX_TESTS@": "0",
        "@MLIR_RUN_ARM_SVE_TESTS@": "0",
        "@MLIR_RUN_X86VECTOR_TESTS@": "0",
        "@MLIR_RUN_CUDA_TENSOR_CORE_TESTS@": "0",
        "@MLIR_INCLUDE_INTEGRATION_TESTS@": "0",
    },
    template = "lit.site.cfg.py.in",
)

