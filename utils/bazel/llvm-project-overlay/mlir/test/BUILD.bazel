        "@LLVM_TOOLS_DIR@": package_path("//llvm:BUILD"),
        "@ENABLE_SHARED@": "1",
        "@ENABLE_ASSERTIONS@": "1",
        "@MLIR_SOURCE_DIR@": package_path("//mlir:BUILD"),
        "@MLIR_TOOLS_DIR@": package_path("//mlir:BUILD"),
        "\"@MLIR_BINARY_DIR@\"": "os.environ[\"TEST_UNDECLARED_OUTPUTS_DIR\"]",
