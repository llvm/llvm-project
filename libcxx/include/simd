#ifndef _LIBCPP_SIMD
#define _LIBCPP_SIMD

#include <__config>

#if _LIBCPP_STD_VER >= 26
#  include <__simd/basic_simd.h>
#  include <__simd/basic_simd_mask.h>
#  include <__simd/simd_flags.h>
#endif

#if !defined(_LIBCPP_HAS_NO_PRAGMA_SYSTEM_HEADER)
#  pragma GCC system_header
#endif

#endif // _LIBCPP_SIMD
