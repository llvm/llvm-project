subroutine sub21()
  real, allocatable,device:: a(:,:), b(:,:)
  real:: s
  integer:: i,j,N=16
  allocate(a(N,N),b(N,N))
  do concurrent(i=1:N, j=1:N) reduce(+:s)
    b(i,j)=a(i,j)**2
    s=s+b(i,j)
  end do
end subroutine

