! Check that cuf.data_transfer are not generated within cuf kernel and do not
! trigger semantic error.
subroutine sub17()
  integer, parameter :: n = 10
  real, device :: adev(n)
  real, device :: bdev(n)
  real :: ahost
  real, managed :: b
  integer :: i

  adev = ahost
  !$cuf kernel do <<<*,*>>>
  do i = 1, n
    ahost = adev(i) * bdev(i) + b
  enddo
end subroutine

! CHECK-LABEL: func.func @_QPsub17()
! CHECK: cuf.kernel<<<*, *>>>
! CHECK-NOT: cuf.data_transfer

subroutine sub18(o)
  integer, device, optional, allocatable :: o(:)
  integer, device, allocatable :: a(:)
  integer, device, pointer :: p(:)
  integer :: b
  integer :: s(1)
  logical :: l

  b = size(a)
  b = lbound(a, dim=1)
  b = ubound(a, dim=1)
  s = shape(a)
  l = allocated(a)
  l = associated(p)
  b = kind(a)
  l = present(o)
end subroutine

