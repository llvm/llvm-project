########## Make rule for test oop130  ########

fcheck.o check_mod.mod: $(SRC)/check_mod.F90
	-$(FC) -c $(FFLAGS) $(SRC)/check_mod.F90 -o fcheck.o

oop130.o:  $(SRC)/oop130.f90 check_mod.mod
	@echo ------------------------------------ building test $@
	-$(FC) -Hx,68,0x80 -c $(FFLAGS) $(LDFLAGS) $(SRC)/oop130.f90 -o oop130.o

oop130: oop130.o fcheck.o
	-$(FC) $(FFLAGS) $(LDFLAGS) oop130.o fcheck.o $(LIBS) -o oop130

oop130.run: oop130
	@echo ------------------------------------ executing test oop130
	oop130
	-$(RM) shape_mod.mod

