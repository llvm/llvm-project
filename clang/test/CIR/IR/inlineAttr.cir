// RUN: cir-tool %s | FileCheck %s -check-prefix=CIR
// RUN: cir-tool %s -cir-to-llvm -o - | FileCheck %s -check-prefix=MLIR

module {
  cir.func @l0() extra( {cir.inline = #cir.inline<no>} )  {
    cir.return
  }
}

