    getgroups;

    # i*
    initgroups;

    # s*
    setgrent; setgroups;
  }
  GLIBC_2.1 {
    # p*
    putgrent;
  }
  GLIBC_2.1.2 {
    # g*
    getgrent_r; getgrgid_r; getgrnam_r;
  }
  GLIBC_2.2.4 {
    # g*
