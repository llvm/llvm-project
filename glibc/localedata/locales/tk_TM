% cyrillic -> latin
% lower case letters

<U0430> "<U0061>"        % а -> a
<U0431> "<U0062>"        % б -> b
<U0432> "<U0077>"        % в -> w
<U0433> "<U0067>"        % г -> g
<U0434> "<U0064>"        % д -> d
<U0435> "<U0065>"        % е -> e (y'e also, but context dependent)
<U0451> "<U00FD><U006F>" % ё -> y'o
<U0436> "<U017E>"        % ж -> z<
<U0497> "<U006A>"        % җ -> j
<U0437> "<U007A>"        % з -> z
<U0438> "<U0069>"        % и -> i
<U0439> "<U00FD>"        % й -> y'
<U043A> "<U006B>"        % к -> k
<U043B> "<U006C>"        % л -> l
<U043C> "<U006D>"        % м -> m
<U043D> "<U006E>"        % н -> n
<U04A3> "<U0148>"        % ң -> n<
<U043E> "<U006F>"        % о -> o
<U04E9> "<U00F6>"        % ө -> o:
<U043F> "<U0070>"        % п -> p
<U0440> "<U0072>"        % р -> r
<U0441> "<U0073>"        % с -> s
<U0442> "<U0074>"        % т -> t
<U0443> "<U0075>"        % у -> u
<U04AF> "<U00FC>"        % ү -> u:
<U0444> "<U0066>"        % ф -> f
<U0445> "<U0068>"        % х -> h
<U0446> "<U0074><U0073>" % ц -> ts
<U0447> "<U00E7>"        % ч -> c,
<U0448> "<U015F>"        % ш -> s,
<U044A> ""               % ъ hard sign omitted
<U044B> "<U0079>"        % ы -> y
<U044C> ""               % ь soft sign omitted.
<U044D> "<U0065>"        % э -> e
<U04D9> "<U00E4>"        % ә -> a:
<U044E> "<U00FD><U0075>" % ю -> y'u
<U044F> "<U00FD><U0061>" % я -> y'a

% upper case letters

<U0410> "<U0041>"        % А -> a
<U0411> "<U0042>"        % Б -> b
<U0412> "<U0057>"        % В -> w
<U0413> "<U0047>"        % Г -> g
<U0414> "<U0044>"        % Д -> d
<U0415> "<U0045>"        % Е -> e (y'e also, but context dependent)
<U0401> "<U00DD><U006F>" % Ё -> y'o
<U0416> "<U017D>"        % Ж -> z<
<U0496> "<U004A>"        % Җ -> j
<U0417> "<U005A>"        % З -> z
<U0418> "<U0049>"        % И -> i
<U0419> "<U00DD>"        % Й -> y'
<U041A> "<U004B>"        % К -> k
<U041B> "<U004C>"        % Л -> l
<U041C> "<U004D>"        % М -> m
<U041D> "<U004E>"        % Н -> n
<U04A2> "<U0147>"        % Ң -> n<
<U041E> "<U004F>"        % О -> o
<U04E8> "<U00D6>"        % Ө -> o:
<U041F> "<U0050>"        % П -> p
<U0420> "<U0052>"        % Р -> r
<U0421> "<U0053>"        % С -> s
<U0422> "<U0054>"        % Т -> t
<U0423> "<U0055>"        % У -> u
<U04AE> "<U00DC>"        % Ү -> u:
