<U0425> "<U0048>"        % Х -> h
<U0426> "<U0054><U0073>" % Ц -> ts
<U0427> "<U00C7>"        % Ч -> c,
<U0428> "<U015E>"        % Ш -> s,
<U042A> ""               % Ъ hard sign omitted
