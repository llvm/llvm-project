collating-element <nW> from "nW"
collating-element <Nw> from "Nw"
collating-element <NW> from "NW"
collating-symbol  <ny-digraph>
collating-element <ny> from "ny"
collating-element <nY> from "nY"
collating-element <Ny> from "Ny"
collating-element <NY> from "NY"
collating-symbol  <i-dot-below>
collating-element <i-dot-below-combining>  from "i<U0323>"
collating-element <I-dot-below-combining>  from "I<U0323>"
collating-symbol  <o-dot-below>
