        "<U0414><U0443><U0448><U0430><U043D><U0431><U0430>";/
        "<U0421><U0435><U0448><U0430><U043D><U0431><U0430>";/
        "<U0427><U043E><U0440><U0448><U0430><U043D><U0431><U0430>";/
