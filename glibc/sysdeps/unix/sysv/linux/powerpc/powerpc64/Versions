libc {
  GLIBC_2.2 {
    # functions used in other libraries

    # g*
    glob64;

    # New rlimit interface
    getrlimit; setrlimit; getrlimit64; setrlimit64;

    # r*
    readdir64; readdir64_r;

    # s*
    scandir64;
  }
  GLIBC_2.3.4 {
    getcontext;
    longjmp;
    setcontext;
    siglongjmp;
    swapcontext;
  }
  GLIBC_2.17 {
    __ppc_get_timebase_freq;
  }
}
