libc {
  GLIBC_2.0 {
    # f*
    ftok;

    # m*
    msgctl; msgget; msgrcv; msgsnd;

    # s*
    semctl; semget; semop; shmat; shmctl; shmdt; shmget;
  }
  GLIBC_2.3.3 {
    # Non-standard function.
    semtimedop;
  }
  GLIBC_PRIVATE {
    # Cancellation point entries.
    __libc_msgrcv; __libc_msgsnd;
  }
}
